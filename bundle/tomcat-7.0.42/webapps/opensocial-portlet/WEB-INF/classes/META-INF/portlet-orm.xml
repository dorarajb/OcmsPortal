				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="gadgetKey" />
			<basic name="serviceName" />
			<basic name="moduleId" />
			<basic name="accessToken" />
			<basic name="tokenName" />
			<basic name="tokenSecret" />

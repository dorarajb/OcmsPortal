<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.2.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_2_0.dtd">

<routes>
	<route>
		<pattern>/rss/{calendarId}</pattern>
		<ignored-parameter name="p_p_cacheability" />
		<implicit-parameter name="p_p_lifecycle">2</implicit-parameter>
		<implicit-parameter name="p_p_resource_id">calendarBookingsRSS</implicit-parameter>
	</route>
	<route>
		<pattern>/event/{calendarBookingId}</pattern>
		<implicit-parameter name="mvcPath">/view_calendar_booking.jsp</implicit-parameter>
	</route>
</routes>